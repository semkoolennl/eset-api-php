{
    "name": "semkoolen/eset-api-php",
    "description": "Eset API client library for PHP.",
    "type": "library",
    "require": {
        "php": ">=5.7",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "composer/ca-bundle": "^1.1",
        "guzzlehttp/guzzle": "^6.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5 || ^7.1 || ^8.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Eset\\Api\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sem Koolen",
            "email": "sem.koolennl@gmail.com"
        }
    ]
}
